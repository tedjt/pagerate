{
  "name": "Page Ranker",
  "description": "Community rank the quality of the current page",
  "version": "0.1",
  "permissions": [
    //"tabs",
    //"<all_urls>",
    "activeTab"
  ],
  /*
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  */
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "web_accessible_resources": ["iframe.html", "firebase_test.css", "firebase_test.js"],
  "content_security_policy": "script-src 'self' https://*.googleapis.com https://cdn.firebase.com; object-src 'self'",
  "manifest_version": 2
}
